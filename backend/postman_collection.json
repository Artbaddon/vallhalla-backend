		{
			"name": "00_Authentication",
			"item": [
				{
					"name": "Check Server Status",
					"request": {
						"method": "GET",
						"url": "{{baseUrl}}"
					},
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Server is running\", function() {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					]
				},
				{
					"name": "Login (Get Token)",
					"request": { 
						"method": "POST", 
						"header": [{"key": "Content-Type", "value": "application/json"}], 
						"body": { 
							"mode": "raw", 
							"raw": "{\n  \"username\": \"admin\",\n  \"password\": \"12345678\"\n}" 
						}, 
						"url": "{{baseUrl}}/auth/login" 
					},
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = JSON.parse(responseBody);",
									"if (jsonData && jsonData.token) {",
									"    pm.collectionVariables.set('authToken', jsonData.token);",
									"    console.log('Token saved to variable: authToken');",
									"    pm.test(\"Token saved successfully\", function() {",
									"        pm.expect(pm.collectionVariables.get('authToken')).to.not.be.empty;",
									"    });",
									"} else {",
									"    console.log('No token found in response');",
									"    pm.test(\"No token found in response\", function() {",
									"        pm.expect.fail('Authentication failed - no token received');",
									"    });",
									"}"
								],
								"type": "text/javascript"
							}
						}
					]
				},
				{
					"name": "Validate Token",
					"request": { 
						"method": "GET", 
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{authToken}}",
								"type": "text"
							}
						],
						"url": "{{baseUrl}}/auth/validate-token" 
					},
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Token is valid\", function() {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					]
				}
			]
		},
